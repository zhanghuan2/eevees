<div class="right-button">
  <button class="btn btn-primary btn-medium addRow">增加外联行</button>
  <button class="btn btn-primary btn-medium addRow insideRow">增加内联行</button>
</div>
<h2 style="font-size: 24px;border-bottom: 1px solid #dddddd;padding-bottom: 10px;">组件</h2>
<ul class="right-page-ul">

{{#each this}}
  <li class="show-title showmore" title="{{name}}" >
    <p class="title">{{@key}}</p>
    <p class="title-msg">{{this.msg}}</p>
    <ul class="show-ele">
      {{#each this.include}}
        <li class="showmore lastli">
          <p class="last-title">{{@key}}</p>
          <ul>
            <p>{{this.msg}}</p>
            <a href="javascript:void(0)" data-path="{{this.path}}">
              &lt;&lt; 拖拽使用
            </a>
            <div class="review-img">
              {{#insertComps 'zcyEvE/comps/' this.path}}{{/insertComps}}
            </div>
          </ul>
        </li>
      {{/each}}
    </ul>
  </li>
{{/each}}
</ul>


