<table>
  <tbody>
  <tr>
    <td width="20%">
      pageID：
    </td>
    <td width="40%">
      <input type="text" name="pageid" class="form-control"/>
    </td>
    <td width="40%"></td>
  </tr>
  <tr>
    <td width="20%">
      页面名称：
    </td>
    <td>
      <input type="text" name="pagename" class="form-control"/>
    </td>
    <td width="60%"></td>
  </tr>
  <tr>
    <td width="20%">
      页面链接：
    </td>
    <td>
      <input type="text" name="href" class="form-control"/>
    </td>
    <td width="60%"></td>
  </tr>
  <tr>
    <td>
      模板选择：
    </td>
    <td>
      <select>
        <option value="hq">注册会计网</option>
      </select>
    </td>
    <td>
      <select name="coms">
        <option value="">请选择...</option>
        <option value="main">首页</option>
        {{#each this}}
          <option value="{{this}}">{{@key}}</option>
        {{/each}}
      </select>
    </td>
  </tr>
  </tbody>
</table>